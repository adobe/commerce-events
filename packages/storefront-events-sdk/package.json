{
    "name": "@adobe/magento-storefront-events-sdk",
    "version": "1.9.0",
    "description": "SDK for working with events on an Adobe Commerce storefront",
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist/"
    ],
    "author": {
        "name": "Adobe",
        "url": "https://www.adobe.com/"
