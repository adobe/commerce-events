        "babel-loader": "^8.2.5",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.0.1",
        "eslint": "^7.32.0",
        "eslint-config-custom": "*",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^26.6.3",
        "prettier": "^2.6.2",
        "sass-loader": "^11.1.1",
        "standard-version": "^9.5.0",
        "tsconfig": "*",
        "ts-jest": "^26.5.6",
        "ts-loader": "^9.3.0",
        "typescript": "^4.7.2",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.0",
        "webpack-merge": "^5.8.0"
